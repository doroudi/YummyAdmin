{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "build": "vite build --base=/ --mode mocking",
    "dev": "vite --open --port 7000  --mode development",
    "dev:mock": "vite --open --port 7000 --mode mocking",
    "test": "vitest",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "check": "biome check --apply-unsafe .",
    "prepare": "husky || true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm check . && git add -A ."
    }
  },
  "dependencies": {
    "@unhead/vue": "^1.11.20",
    "@unocss/reset": "^66.3.2",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^10.11.1",
    "@vueuse/head": "^1.3.1",
    "apexcharts": "^4.7.0",
    "axios": "^0.27.2",
    "canvas-confetti": "^1.9.3",
    "moment": "^2.30.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-demi": "^0.14.10",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0",
    "vue3-map-chart": "^0.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@faker-js/faker": "^8.4.1",
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@types/nprogress": "^0.2.3",
    "@vicons/fluent": "^0.12.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue-macros/volar": "3.0.0-beta.15",
    "@vue/test-utils": "^2.4.6",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "husky": "^9.1.7",
    "lodash": "^4.17.21",
    "msw": "^2.10.2",
    "naive-ui": "^2.42.0",
    "pinia-plugin-persistedstate": "^4.4.1",
    "pnpm": "^9.15.9",
    "sass": "^1.89.2",
    "taze": "^0.11.4",
    "typescript": "^5.8.3",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-router": "^0.8.8",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^2.2.10"
  },
  "msw": {
    "workerDirectory": ["public"]
  }
}
